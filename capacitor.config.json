{
  "appId": "com.runstr.app",
  "appName": "Runstr",
  "webDir": "dist",
  "plugins": {
    "StatusBar": {
      "style": "Dark",
      "backgroundColor": "#0F1419",
      "androidStatusBarColor": "#0F1419"
    },
    "BackgroundGeolocation": {
      "locationProvider": 3,
      "desiredAccuracy": 10,
      "stationaryRadius": 50,
      "distanceFilter": 5,
      "debug": false,
      "stopOnTerminate": false,
      "startForeground": true,
      "foregroundServiceType": "location"
    },
    "App": {
      "appUrlOpen": {
        "enabled": true
      }
    }
  },
  "android": {
    "useLegacyBridge": true,
    "backgroundColor": "#0F1419",
    "webContentsDebuggingEnabled": false,
    "scheme": "https",
    "hostname": "runstr.app",
    "customSchemes": ["runstr"]
  }
}
